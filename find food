local TweenService = game:GetService("TweenService")
local tweenInfo = TweenInfo.new(1.2, Enum.EasingStyle.Linear)

local items = workspace.Items:GetChildren()

for i = 0, math.min(10000, #items) do  -- ensure we don't go past available items
    local item = items[i]
    if item then
        local handle = item:FindFirstChild("Handle")
        if handle then
            -- Replace with your target position
            local targetCFrame = CFrame.new(1.1549145, 8.30180168, 21.677948, -1, 0, 0, 0, 1, 0, 0, 0, -1w)
            
            -- Create and play tween
            local tween = TweenService:Create(handle, tweenInfo, {CFrame = targetCFrame})
            tween:Play()
        end
    end
end
